{
  "const equals": {
    "prefix": "c=",
    "body": "const ${1:name} = $2;"
  },
  "let equals": {
    "prefix": "l=",
    "body": "let ${1:name} = $2;"
  },
  "named function": {
    "prefix": "fn",
    "body": "function ${1:name}(${2:arguments}) {\n\t${0}\n}",
    "luasnip": {
      "priority": 2000
    }
  },
  "Constructor": {
    "prefix": "ctor",
    "body": [
      "constructor($1) {$0}"
    ],
    "luasnip": {
      "priority": 2000
    }
  },
  "getContext('2d')": {
    "prefix": "g2d",
    "body": "${1:canvas}.getContext('2d')"
  }
}
